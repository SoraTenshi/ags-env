{
    "compilerOptions": {
        "target": "ES2022",
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "lib": [
            "ES2022"
        ],
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "noImplicitAny": true,
        "noEmit": true,
        "baseUrl": ".",
        "typeRoots": [
            "./types/**",
            "./node_modules/@girs"
        ],
        "skipLibCheck": true,
    }
}
